{
  "name": "camp",
  "version": "1.0.0",
  "description": "ai practice",
  "main": "index.js",
  "module": "es2020",
  "author": "Artem_L",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "ai-start": "node --loader ts-node/esm -r dotenv/config open-ai/index.ts",
    "l-chain": "tsx -r dotenv/config langchain/index.ts",
    "build": "tsc",
    "server": "NODE_ENV=development node dist/server/app.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/server/app.js\"",
    "commitmsg": "commitlint --edit $HUSKY_GIT_PARAMS",
    "postinstall": "husky install",
    "check:all": "eslint --fix && prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@types/axios": "0.14.0",
    "@types/bunyan": "1.8.8",
    "@types/cors": "2.8.17",
    "@types/debug": "4.1.8",
    "@types/express": "4.17.17",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "concurrently": "8.2.0",
    "eslint": "8.43.0",
    "eslint-plugin-typescript": "0.14.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "nodemon": "^3.1.7",
    "ts-node": "10.9.1",
    "tsx": "^4.19.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "yarn run commitmsg"
    }
  },
  "nodemonConfig": {
    "watch": [
      "*.js"
    ]
  },
  "dependencies": {
    "@langchain/community": "^0.3.7",
    "@langchain/openai": "^0.3.11",
    "@langchain/pinecone": "^0.1.1",
    "@pinecone-database/pinecone": "^3.0.3",
    "axios": "^1.7.7",
    "bunyan": "1.8.15",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.0",
    "js-tiktoken": "^1.0.15",
    "langchain": "^0.3.2",
    "openai": "^4.67.3",
    "prettier": "^3.3.3"
  },
  "resolutions": {
    "@langchain/core": "^0.3.0"
  },
  "overrides": {
    "@langchain/core": "^0.3.0"
  }
}
